§addType(eType.Class, "Player")
§addMain
while (true) {
	§vardecl(Int32, "m", 0);
	§vardecl(Int32, "dest", 0);
	§vardecl(Int32, "idx", 0);
	§vardecl(Int32, "i", 0);
	for(i = 0; < i 8; i++){
		§readLine(m);
		if(> m dest){
			dest = m;
			idx = i;
		}
	}
	§writeLine(idx);
}
§endMain
§endType

